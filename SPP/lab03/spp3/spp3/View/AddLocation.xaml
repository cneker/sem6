<Window x:Class="spp3.View.AddLocation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:spp3.View"
        mc:Ignorable="d"
        Title="AddLocation" Height="345" Width="569">
    <Grid>
        <TextBlock Margin="9,34,378,154" Text="Name"/>
        <TextBox x:Name="Name" HorizontalAlignment="Left" Height="23" Margin="105,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <TextBlock Margin="9,67,378,121" Text="Country"/>
        <TextBox x:Name="Country" HorizontalAlignment="Left" Height="23" Margin="105,68,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <TextBlock Margin="10,104,485,164" Text="Race"/>
        <ComboBox x:Name="Race" Margin="105,108,246,164"/>
        <Label x:Name="Label" Visibility="Hidden" Content="SELECT RACE (think how)" Margin="360,122,40,121"></Label>
        <Button x:Name="insertBtn" Content="Add" HorizontalAlignment="Left" Margin="9,208,0,0" VerticalAlignment="Top" Width="75" Click="insertBtn_Click"/>
    </Grid>
</Window>
