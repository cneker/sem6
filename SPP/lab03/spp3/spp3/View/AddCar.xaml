<Window x:Class="spp3.View.AddCar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:spp3"
        mc:Ignorable="d"
        Title="AddCar" Height="228" Width="406">
    <Grid>
        <TextBlock Margin="9,34,326,121" Text="Model"/>
        <TextBox x:Name="Mark" HorizontalAlignment="Left" Height="23" Margin="105,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="262" />
        <Button x:Name="insertBtn" Content="Add" HorizontalAlignment="Left" Margin="18,96,0,0" VerticalAlignment="Top" Width="75" Click="insertBtn_Click"/>
    </Grid>
</Window>
